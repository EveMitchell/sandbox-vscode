{
  "name": "sandbox",
  "displayName": "Sandbox",
  "description": "Instantly test snippets of JS/HTML/CSS using VS Code.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "playground",
    "sandbox",
    "scratchpad",
    "repl",
    "javascript"
  ],
  "activationEvents": [
    "onFileSystem:sandbox",
    "onCommand:sandbox.newSandbox"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "New Sandbox",
        "category": "Sandbox",
        "command": "sandbox.newSandbox"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "jsdom": "15.1.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jsdom": "^12.2.4",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0"
  }
}
